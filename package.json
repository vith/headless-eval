{
	"name": "headless-eval",
	"version": "1.3.1",
	"description": "CLI tool to evaluate a JS snippet in a webpage context with headless chrome",
	"homepage": "https://github.com/vith/headless-eval",
	"repository": {
		"type": "git",
		"url": "https://github.com/vith/headless-eval.git"
	},
	"bin": {
		"headless-eval": "dist/cli.js"
	},
	"main": "dist/lib.js",
	"scripts": {
		"build": "tsc",
		"test": "ava",
		"prepare": "npm run build"
	},
	"author": "Jason Papakostas <vithos@gmail.com>",
	"license": "ISC",
	"dependencies": {
		"commander": "^6.0.0",
		"puppeteer": "^5.0.0",
		"read-pkg-up": "^7.0.1"
	},
	"devDependencies": {
		"@koa/router": "9.4.0",
		"@types/koa": "2.11.4",
		"@types/koa__router": "8.0.2",
		"@types/node": "13.13.15",
		"@types/npm": "2.0.31",
		"@types/puppeteer": "3.0.1",
		"ava": "3.12.1",
		"execa": "4.0.3",
		"get-port": "5.1.1",
		"http-status-codes": "1.4.0",
		"koa": "2.13.0",
		"npm": "6.14.8",
		"pkg-dir": "4.2.0",
		"ts-node": "8.10.2",
		"typescript": "4.0.2"
	},
	"ava": {
		"extensions": [
			"ts"
		],
		"require": [
			"ts-node/register"
		]
	}
