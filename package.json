{
	"name": "headless-eval",
	"version": "1.3.2",
	"description": "CLI tool to evaluate a JS snippet in a webpage context with headless chrome",
	"homepage": "https://github.com/vith/headless-eval",
	"repository": {
		"type": "git",
		"url": "https://github.com/vith/headless-eval.git"
	},
	"bin": {
		"headless-eval": "dist/cli.js"
	},
	"main": "dist/lib.js",
	"scripts": {
		"build": "tsc",
		"test": "ava",
		"prepare": "npm run build"
	},
	"author": "Jason Papakostas <vithos@gmail.com>",
	"license": "ISC",
	"dependencies": {
		"commander": "^7.0.0",
		"puppeteer": "^9.0.0",
		"read-pkg-up": "^7.0.1"
	},
	"devDependencies": {
		"@koa/router": "10.0.0",
		"@types/koa": "2.13.3",
		"@types/koa__router": "8.0.4",
		"@types/node": "13.13.51",
		"@types/npm": "2.0.31",
		"@types/puppeteer": "5.4.3",
		"ava": "3.15.0",
		"execa": "5.0.1",
		"get-port": "5.1.1",
		"http-status-codes": "2.1.4",
		"koa": "2.13.1",
		"npm": "7.15.1",
		"pkg-dir": "5.0.0",
		"ts-node": "9.1.1",
		"typescript": "4.3.2"
	},
	"ava": {
		"extensions": [
			"ts"
		],
		"require": [
			"ts-node/register"
		]
	}
