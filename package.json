{
	"name": "headless-eval",
	"version": "1.2.0",
	"description": "CLI tool to evaluate a JS snippet in a webpage context with headless chrome",
	"homepage": "https://github.com/vith/headless-eval",
	"repository": {
		"type": "git",
		"url": "https://github.com/vith/headless-eval.git"
	},
	"bin": {
		"headless-eval": "dist/cli.js"
	},
	"main": "dist/lib.js",
	"scripts": {
		"build": "tsc",
		"test": "ava",
		"prepare": "npm run build"
	},
	"author": "Jason Papakostas <vithos@gmail.com>",
	"license": "ISC",
	"dependencies": {
		"commander": "^4.0.1",
		"puppeteer": "^2.0.0",
		"read-pkg-up": "^7.0.1"
	},
	"devDependencies": {
		"@koa/router": "8.0.5",
		"@types/koa": "2.11.0",
		"@types/koa__router": "8.0.2",
		"@types/node": "13.1.6",
		"@types/puppeteer": "2.0.0",
		"ava": "2.4.0",
		"get-port": "5.1.0",
		"http-status-codes": "1.4.0",
		"koa": "2.11.0",
		"ts-node": "8.6.0",
		"typescript": "3.7.4"
	},
	"ava": {
		"compileEnhancements": false,
		"extensions": [
			"ts"
		],
		"require": [
			"ts-node/register"
		]
	}
}
