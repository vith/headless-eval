	},
	"author": "Jason Papakostas <vithos@gmail.com>",
	"license": "ISC",
	"dependencies": {
		"commander": "^8.3.0",
		"puppeteer": "^9.0.0",
		"read-pkg-up": "^7.0.1"
	},
	"devDependencies": {
		"@koa/router": "10.1.1",
		"@types/koa": "2.13.4",
		"@types/koa__router": "^8.0.11",
		"@types/node": "^17.0.6",
		"@types/npm": "^7.19.0",
		"@types/puppeteer": "5.4.4",
		"ava": "3.15.0",
		"execa": "5.1.1",
		"get-port": "5.1.1",
		"http-status-codes": "^2.2.0",
		"koa": "^2.13.4",
		"pkg-dir": "5.0.0",
		"ts-node": "^10.4.0",
		"typescript": "^4.5.4"
